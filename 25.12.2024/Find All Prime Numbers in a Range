def is_prime(n: int) -> bool:
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def primes_in_range(limit: int) -> list:
    return [n for n in range(2, limit + 1) if is_prime(n)]

# Test case
print(primes_in_range(20))  # [2, 3, 5, 7, 11, 13, 17, 19]
